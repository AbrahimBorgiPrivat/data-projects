                "db_type":"DB_TYPE"},
  "schema": "gamma_db",
  "table_name": "mobilepay",
  "create_table_if_not_exist": true,
  "fields_dict": {
    "id":               { "type": "TEXT", "primary_key": true, "autoincrement": false },
    "date":             { "type": "DATE" },
    "timestamp_iso":    { "type": "TIMESTAMP" },
    "amount":           { "type": "DOUBLE_PRECISION" },
    "message":          { "type": "TEXT" },
    "transaction_type": { "type": "TEXT" },
    "transfer_ref":     { "type": "TEXT" },
    "transfer_date":    { "type": "DATE" },
    "merchant_name":    { "type": "TEXT" },
    "receiver_acct":    { "type": "TEXT" },
    "payment_tx_id":    { "type": "TEXT" },
    "payner_name":      { "type": "TEXT" },
    "payer_phone":      { "type": "TEXT" }
  },
  "update_fields": [
    "date",
    "timestamp_iso",
    "amount",
    "message",
    "transaction_type",
    "transfer_ref",
    "transfer_date",
    "merchant_name",
    "receiver_acct",
    "payment_tx_id",
    "payner_name",
    "payer_phone"
  ],
  "not_included_in_update_fields": [],
  "path": "gamma/csv/mobilepay.csv",
