  "subset": [
    "Date",
    "Timestamp",
    "Amount",
    "Message",
    "Transaction Type",
    "Transfer Reference",
    "Transfer Date",
    "Merchant Name",
    "Receiver Account",
    "Payment Transaction ID",
    "User Name",
