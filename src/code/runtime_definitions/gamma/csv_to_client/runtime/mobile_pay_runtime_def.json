    { "former_key": "Transfer Date",          "new_key": "transfer_date",    "type": "date_iso",      "fmt": "%d-%m-%Y" },
    { "former_key": "Merchant Name",          "new_key": "merchant_name",    "type": "string" },
    { "former_key": "Receiver Account",       "new_key": "receiver_acct",    "type": "string" },
    { "former_key": "Payment Transaction ID", "new_key": "payment_tx_id",    "type": "string" },
    { "former_key": "User Name",              "new_key": "payner_name",      "type": "string" },
    { "former_key": "User Phone Number",      "new_key": "payer_phone",      "type": "string" }
  ],
  "pk_from": ["payment_tx_id", "transaction_type"],
  "pk_name": "id",
  "defaults": { "source": "mobilepay" },
